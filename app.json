{
  "name": "Sample Backend NodeJS",
  "website": "https://www.hypertrack.com/",
  "success_url": "/welcome",
  "description": "Sample NodeJS server integration for Placeline application",
  "addons": ["mongolab:sandbox", "papertrail:choklad", "cloudamqp:lemur"],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    },
    "worker": {
      "quantity": 1,
      "size": "free"
    },
    "clock": {
      "quantity": 1,
      "size": "free"
    }
  },
  "env": {
    "HT_ACCOUNT_ID": {
      "description": "Your HyperTrack AccountId from https://dashboard.hypertrack.com/setup",
      "value": ""
    },
    "HT_SECRET_KEY": {
      "description": "Your HyperTrack SecretKey from https://dashboard.hypertrack.com/setup",
      "value": ""
    }
  },
  "repository": "https://github.com/hypertrack/sample-backend-nodejs",
  "logo": "https://dashboard.hypertrack.com/assets/hypertrack-bw.svg",
  "keywords": [
    "sample",
    "hypertrack",
    "placeline",
    "integration",
    "mongodb",
    "mongoose",
    "heroku"
  ]
}
